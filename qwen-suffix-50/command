uv run main.py --model qwen2.5:7b --defense-mechanism objective --attack-mode suffix-attack --dataset-size 50 --max-attempts 10 --output-path qwen-suffix-attack-objective && uv run main.py --model qwen2.5:7b --defense-mechanism noop --attack-mode suffix-attack --dataset-size 50 --max-attempts 10 --output-path qwen-suffix-attack-no-defense